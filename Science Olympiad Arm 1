task main()
{
	while(true){
		motor[port1] = vexRT[Ch1]; //Arm motor lower
		motor[port2] = vexRT[Ch2]; //Arm motor middle
		
		if(vexRT[Btn8R]){ //Arm motor higher
			motor[port3] = 127;
		}else if(vexRT[Btn8D]){
		 	motor[port3] = -127;
		}
		
		if(vexRT[Btn5U]){ //Horizontal movement
			motor[port4] = 127;
		}else if(Btn6U){
			motor[port4] = -127;
		}
		
		if(vexRT[Btn8U]){ //Possible claw movement
			motor[port5] = 127;
		}else if(Btn8L){
			motor[port5] = -127;
		}
	}
}
